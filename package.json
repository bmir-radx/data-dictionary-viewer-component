  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@r2wc/react-to-web-component": "^2.0.4",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.0",
